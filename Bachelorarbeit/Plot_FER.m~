openfig('Cap_1QAM.fig');
hold on;
title('FER vs. Capacity of QAM');
load('snr_FER_1_2_QAM.mat');
plot(snr_FER,2,'r*');
load('snr_FER_2_3_A_QAM.mat');
plot(snr_FER,(4*2/3),'r*');
load('snr_FER_3_4_A_QAM.mat');
plot(snr_FER,3,'r*');
load('snr_FER_5_6_QAM.mat');
plot(snr_FER,(4*5/6),'r*');

legend('16-QAM','R = 1/2', 'R = 2/3', 'R = 3/4', 'R = 5/6');
xlim([-10,20]);
ylim([0, 4.5]);